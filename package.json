        "build-storybook": "storybook build -c ./config/storybook",
        "prepare": "husky install",
        "generate:slice": "node ./scripts/createSlice/index.js",
        "ts-node": "ts-node .\\scripts\\refactoring\\createPublicApiForSharedUi.ts",
        "remove-feature": "npx ts-node ./scripts/remove-feature.ts isAppRedisigned on"
    },
    "lint-staged": {
        "**/*.{ts,tsx}": [
            "prettier --list-different",
